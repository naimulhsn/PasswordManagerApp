<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skillhunternaim.anotherloser.passwordmanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetMasterPassword" />
        <activity android:name=".EnterMasterPassword" />
        <activity
            android:name=".AllPasswords"
            android:label="All Saved Passwords List"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NewPass"></activity>
    </application>

</manifest>